/*first programe usually start from 'hello world'*/
#include<iostream>
using namespace std;
int main()
{
	char hello[4];
	char world[5];
	hello[0] = 'H';
	hello[1] = 'E';
	hello[2] = 'L';
	hello[3] = 'L';
	hello[4] = 'O';
	world[0] = ' ';
	world[1] = 'W';
	world[2] = 'O';
	world[3] = 'R';
	world[4] = 'L';
	world[5] = 'D';
	cout << hello[0] << hello[1] << hello[2] << hello[3] << hello[4] << world[0] << world[1] << world[2] << world[3] << world[4] << world[5];
	return 0;
}
